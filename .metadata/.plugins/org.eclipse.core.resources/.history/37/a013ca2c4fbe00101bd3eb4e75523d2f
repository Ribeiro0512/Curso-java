package fundamentos;

public class Inferencia {

	public static void main(String[] args) {
		
		double a = 4.5;
		System.out.println(a);
		
		var b = 4.5; //Através da inferencia o java define de acordo com o valor da variável
		System.out.println(b);
		
		var c = "texto";
		System.out.println(c);
		c = "Outro texto";
		System.out.println(c);
	}
}
