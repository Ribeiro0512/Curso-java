package fundamentos;

public class Temperatura {
	
	public static void main(String[] args) {
		// ( °F - 32) x 5/9 = °C
		
		final int AJUSTE = 32;
		final double MULTIPLICADOR = 5.0 / 9.0;
		double f = 80;
		
		double celcius = (f - AJUSTE) * MULTIPLICADOR;
		System.out.println("A temperatura em Celcius é: " + f.2celcius);
	}
}
